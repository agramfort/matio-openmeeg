#ifndef MATIO_PUBCONF_H
#define MATIO_PUBCONF_H 1

/* Default file format */
#define MAT_FT_DEFAULT @MAT_FT_DEFAULT@

/* Define to 1 if you have the <stdint.h> header file. */
#cmakedefine MATIO_HAVE_STDINT_H 1

/* Define to 1 if you have the <inttypes.h> header file. */
#cmakedefine MATIO_HAVE_STDINT_H 1

#cmakedefine MAT_FT_DEFAULT @MAT_FT_DEFAULT@

#cmakedefine HAVE_MAT_INT64_T 1
#cmakedefine HAVE_MAT_INT32_T 1
#cmakedefine HAVE_MAT_INT16_T 1
#cmakedefine HAVE_MAT_INT8_T 1
#cmakedefine HAVE_MAT_UINT64_T 1
#cmakedefine HAVE_MAT_UINT32_T 1
#cmakedefine HAVE_MAT_UINT16_T 1
#cmakedefine HAVE_MAT_UINT8_T 1

#include <inttypes.h>
#include <stdint.h>

typedef int64_t  mat_int64_t;
typedef uint64_t mat_uint64_t;
typedef int32_t  mat_int32_t;
typedef uint32_t mat_uint32_t;
typedef int16_t  mat_int16_t;
typedef uint16_t mat_uint16_t;
typedef int8_t   mat_int8_t;
typedef uint8_t  mat_uint8_t;

#endif /* MATIO_PUBCONF_H */
